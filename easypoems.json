[
  {
    "title": "憫農二首·其二",
    "content": [
      "鋤禾日當午",
      "汗滴禾下土",
      "誰知盤中餐",
      "粒粒皆辛苦？"
    ]
  },
  {
    "title": "靜夜思",
    "content": [
      "床前明月光",
      "疑是地上霜",
      "舉頭望明月",
      "低頭思故鄉"
    ]
  },
  {
    "title": "春曉",
    "content": [
      "春眠不覺曉",
      "處處聞啼鳥",
      "夜來風雨聲",
      "花落知多少"
    ]
  },
  {
    "title": "尋隱者不遇",
    "content": [
      "松下問童子",
      "言師採藥去",
      "只在此山中",
      "雲深不知處"
    ]
  },
  {
    "title": "登鸛雀樓",
    "content": [
      "白日依山盡",
      "黃河入海流",
      "欲窮千里目",
      "更上一層樓"
    ]
  },
  {
    "title": "江雪",
    "content": [
      "千山鳥飛絕",
      "萬徑人蹤滅",
      "孤舟蓑笠翁",
      "獨釣寒江雪"
    ]
  },
  {
    "title": "憫農二首·其一",
    "content": [
      "春種一粒粟",
      "秋收萬顆子",
      "四海無閒田",
      "農夫猶餓死"
    ]
  },
  {
    "title": "賦得古原草送別",
    "content": [
      "離離原上草",
      "一歲一枯榮",
      "野火燒不盡",
      "春風吹又生",
      "遠芳侵古道",
      "晴翠接荒城",
      "又送王孫去",
      "萋萋滿別情"
    ]
  },
  {
    "title": "鹿柴",
    "content": [
      "空山不見人",
      "但聞人語響",
      "返景入深林",
      "復照青苔上"
    ]
  },
  {
    "title": "獨坐敬亭山",
    "content": [
      "眾鳥高飛盡",
      "孤雲獨去閒",
      "相看兩不厭",
      "只有敬亭山"
    ]
  },
  {
    "title": "山居秋暝",
    "content": [
      "空山新雨後",
      "天氣晚來秋",
      "明月松間照",
      "清泉石上流",
      "竹喧歸浣女",
      "蓮動下漁舟",
      "隨意春芳歇",
      "王孫自可留"
    ]
  },
  {
    "title": "遊子吟",
    "content": [
      "慈母手中線",
      "遊子身上衣",
      "臨行密密縫",
      "意恐遲遲歸",
      "誰言寸草心",
      "報得三春暉"
    ]
  },
  {
    "title": "鳥鳴澗",
    "content": [
      "人閒桂花落",
      "夜靜春山空",
      "月出驚山鳥",
      "時鳴春澗中"
    ]
  },
  {
    "title": "過故人莊",
    "content": [
      "故人具雞黍",
      "邀我至田家",
      "綠樹村邊合",
      "青山郭外斜",
      "開軒面場圃",
      "把酒話桑麻",
      "待到重陽日",
      "還來就菊花"
    ]
  }
]